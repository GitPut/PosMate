{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": "./",
    "paths": {
      "assets/*": ["assets/*"],
      "components/*": ["components/*"],
      "navigation/*": ["navigation/*"],
      "pages/*": ["pages/*"],
      "state/*": ["state/*"]
    },
  },
  "extends": "expo/tsconfig.base",
  "exclude": ["node_modules", "babel.config.js", "metro.config.js", "jest.config.js", "web-build"]
  // this is new test exclude
}
